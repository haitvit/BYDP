<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui"
	xmlns:f="urn:import:com.org.niteco.se.test.widget">
	<ui:with type="com.org.niteco.se.test.client.FinsResources" field="resource"/>
	<g:StackLayoutPanel unit="EM">
	    <g:stack>
	        <g:customHeader size='1.8'>
	            <f:MenuHeader text="Analyze" imageResource="{resource.albumImages}" />
	        </g:customHeader>
	        <g:LayoutPanel>
	            <g:layer left='0.5em' width='10em' top='0.5em' height='3em'>
	                <f:MenuItem contentName='Snapshot' text='symbol' imageResource="{resource.albumImages}" />
	            </g:layer>
	            <g:layer left='0.5em' width='10em' top='2.3em' height='3em'>
	                <f:MenuItem contentName='Watchlists' text='symbol' imageResource="{resource.albumImages}" />
	            </g:layer>
	        </g:LayoutPanel>
	    </g:stack>
	    <g:stack>
	        <g:customHeader size='1.8'>
	            <f:MenuHeader text="Manage" imageResource="{resource.albumImages}" />
	        </g:customHeader>
	        <g:LayoutPanel>
	            <g:layer left='0.5em' width='10em' top='0.5em' height='3em'>
	                <f:MenuItem contentName='ManageWatchlist' text='Watchlist' imageResource="{resource.albumImages}" />
	            </g:layer>
	        </g:LayoutPanel>
	    </g:stack>
	</g:StackLayoutPanel>
</ui:UiBinder> 